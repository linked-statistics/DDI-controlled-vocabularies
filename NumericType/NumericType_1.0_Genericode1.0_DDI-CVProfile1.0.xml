        <ShortName>NumericType</ShortName>
        <LongName xml:lang="en-US">Numeric Type</LongName>
        <Version>1.0</Version>
        <CanonicalUri>urn:ddi-cv:NumericType</CanonicalUri>
        <CanonicalVersionUri>urn:ddi-cv:NumericType:1.0</CanonicalVersionUri>
