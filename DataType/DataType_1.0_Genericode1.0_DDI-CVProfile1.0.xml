    <SimpleCodeList>
        <Row>
            <Value ColumnRef="Code">
                <SimpleValue>String</SimpleValue>
            </Value>
            <Value ColumnRef="Term">
                <ComplexValue>
                    <ddi-cv:Value xml:lang="en-US">String</ddi-cv:Value>
                </ComplexValue>
            </Value>
            <Value ColumnRef="Definition">
                <ComplexValue>
                    <ddi-cv:Value xml:lang="en-US">Finite sequences of characters. A character is an atomic unit of written communication; it is not further specified except to note that every character has a corresponding Universal Character Set code point (which is an integer).</ddi-cv:Value>
                </ComplexValue>
            </Value>
        </Row>
           <Row>
               <Value ColumnRef="Code">
                   <SimpleValue>NormalizedString</SimpleValue>
               </Value>
               <Value ColumnRef="Term">
