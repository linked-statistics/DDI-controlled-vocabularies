   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/DDICV/AnalysisUnit_1.0#CodeList">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
      <dcterms:title xml:lang="en">Analysis Unit</dcterms:title>
      <skos:notation>AnalysisUnit</skos:notation>
      <dcterms:description xml:lang="en">Describes the entity being analyzed in the study or in the variable.</dcterms:description>
      <dcterms:isVersionOf rdf:resource="http://rdf-vocabulary.ddialliance.org/DDICV/AnalysisUnit#"/>
